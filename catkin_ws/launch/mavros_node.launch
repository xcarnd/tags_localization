<?xml version="1.0"?>
<launch>
    <arg name="fcu_url" value="/dev/ttyS0:57600" />

    <!-- mavros node for px4 stack -->
    <include file="$(find mavros)/launch/px4.launch" />

    <!-- vision pose estimation from mavros_extras -->
    <node pkg="mavros_extras" name="vision_pose_estimation" type="vision_pose_estimation">
        <remap from="~/vision_pose/pose" to="/locator/pose">
    </node>
</launch>
